  publisher = {Springer-Verlag},
  note = 	 {Keywords: Application::Telecommunications},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900789.pdf },
  abstract = {Increasing the channel re-usability is necessary for
reducing the@call-blocking rate in any cellular systems
with limited bandwidth and@large number of subscribers.
To increase the re-usability, we need an efficient channel assignment algorithm that minimizes the sum of blocking
cost and interference cost. We propose a new genetic algorithm for the problem based on the parameter-free GA.  The proposed GA finds a good sequence of codes for a virtual machine that produces channel assignment.
Results are given which show that our
GA, without tedious parameter tuning, produces far better solutions to several practical problems than the existing GAs.
}
}
@InProceedings{ LNCS2439:ID197:pp665,
		  author = 	 { M.G. Arenas, Pierre Collet, A.E. Eiben, M'ark
