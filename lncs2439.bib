@InProceedings{ LNCS2439:ID171:pp288,
		  author = 	 { Nattavut Keerativuttitumrong, Nachol Chaiyaratana, and
				Vara Varavithya},
		    title = 	 {Multi-objective Co-operative Co-evolutionary Genetic Algorithm},
		      booktitle = 	 {Parallel Problem Solving from Nature - PPSN VII, 7th International Conference, Granada, Spain, September 7-11, 2002. Proceedings },
		      pages = 	 { 288 ff.},
  year = 	 {2002},
  editor = 	 {J.-J. Merelo Guerv\'os, P. Adamidis, H.-G. Beyer, J.-L. Fern\'andez-Villaca\~nas, H.-P. Schwefel},
  number = 	 {2439},
  series = 	 {Lecture Notes in Computer Science, LNCS},
  publisher = {Springer-Verlag},
  note = 	 {Keywords: Implementation::Parallel EAs, Technique::Co-evolution, Technique::Multi-objective},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900288.pdf },
  abstract = {This paper presents the integration between two types of genetic algorithm: a multi-objective genetic algorithm (MOGA) and a co-operative co-evolutionary genetic algorithm (CCGA). The resulting algorithm is referred to as a multi-objective co-operative co-evolutionary genetic algorithm or MOCCGA. The integration between the two algorithms is carried out in order to improve the performance of the MOGA by adding the co-operative co-evolutionary effect to the search mechanisms employed by the MOGA. The MOCCGA is benchmarked against the MOGA in six different test cases. The test problems cover six different characteristics that can be found within multi-objective optimisation problems: convex Pareto front, non-convex Pareto front, discrete Pareto front, multi-modality, deceptive Pareto front and non-uniformity in the solution distribution. The simulation results indicate that in overall the MOCCGA is superior to the MOGA in terms of the variety in solutions generated and the closeness of solutions to the true Pareto-optimal solutions. A simple parallel implementation of MOCCGA is described. With an 8-node cluster, the speed up of 2.69 to 4.8 can be achieved for the test problems.}
}
@InProceedings{ LNCS2439:ID207:pp132,
		  author = 	 { David W. Corne,
				Martin J. Oates, and Douglas B. Kell},
		    title = 	 {On Fitness Distributions and Expected Fitness Gains of Parallelised Mutation Operators: Implications for High Mutation Rates and Rate Adaptation in Parallel Evolutionary Algorithms},
		      booktitle = 	 {Parallel Problem Solving from Nature - PPSN VII, 7th International Conference, Granada, Spain, September 7-11, 2002. Proceedings },
		      pages = 	 { 132 ff.},
  year = 	 {2002},
  editor = 	 {J.-J. Merelo Guerv\'os, P. Adamidis, H.-G. Beyer, J.-L. Fern\'andez-Villaca\~nas, H.-P. Schwefel},
  number = 	 {2439},
  series = 	 {Lecture Notes in Computer Science, LNCS},
  publisher = {Springer-Verlag},
  note = 	 {Keywords: Implementation::Parallel EAs, Implementation::Parameter tuning, Technique::Evolutionary computing - general, Technique::Evolutionary computing - miscellaneous},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900132.pdf },
  abstract = {Setting the mutation rate for an evolutionary algorithm (EA) is confounded by dependence on the problem under study and other details. Nevertheless, much theoretical and empirical work has been done which provides generally accepted guidelines, suggesting that 1/L (where L is chromosome length) is likely to be an optimal or near-optimal rate, and a good starting point for fine-tuning. Here we investigate mutation rates in the context of large-population-parallelism. We justify the claim that considerable speedup can be obtained with raised rates (above that achieved purely as a result of parallelisation), both theoretically and empirically. The simple underlying theory notices that parallelisation favourably alters the fitness distribution of the mutation operator. We derive an expression which sets out how this is changed in terms of the level of parallelisation, and we describe a mutation rate adaptation technique for parallel EAs which uses the derived fitness distribution equations, and compare it with a range of alternatives. Preliminary results suggest that this method can be of strong value in parallel EAs, leading to reliably reduced numbers of generations to find good-quality solutions.}
}
@InProceedings{ LNCS2439:ID92:pp153,
		  author = 	 { C.
				Herv'as-Mart'inez, D. Ortiz-Boyer, and N.
				Garc'ia-Pedrajas},
		    title = 	 {Theoretical Analysis of the Confidence Interval Based Crossover
for Real-Coded Genetic Algorithms},
		      booktitle = 	 {Parallel Problem Solving from Nature - PPSN VII, 7th International Conference, Granada, Spain, September 7-11, 2002. Proceedings },
		      pages = 	 { 153 ff.},
  year = 	 {2002},
  editor = 	 {J.-J. Merelo Guerv\'os, P. Adamidis, H.-G. Beyer, J.-L. Fern\'andez-Villaca\~nas, H.-P. Schwefel},
  number = 	 {2439},
  series = 	 {Lecture Notes in Computer Science, LNCS},
  publisher = {Springer-Verlag},
  note = 	 {Keywords: Activities::Mathematical modelling/curve fitting, Technique::Evolutionary computing - general, Theory of EC::Performance analysis},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900153.pdf },
  abstract = {  In this paper we study some theoretical aspects of a new crossover
  operator for real-coded genetic algorithms based on the statistical
  features of the best individuals of the population. This crossover is
  based on defining a confidence interval for a localization estimator
  using the $L_2$ norm. From this confidence interval we obtain three
  parents: the localization estimator and the lower and upper limits
  of the confidence interval. In this paper we analyze the mean and
  variance of the population when this crossover is applied, studying
  the behavior of the distribution of the fitness of the individuals
  in a problem of optimization. We also make a comparison of our
  crossover with the crossovers BLX-$\alpha$ and UNDX-$m$, showing the
  robustness of our operator. }
}
