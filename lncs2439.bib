@InProceedings{ LNCS2439:ID193:pp361,
		  author = 	 {
				Michael Emmerich, Alexios Giotis, Mutlu \"Ozdemir, Thomas B\"ack, and
				Kyriakos Giannakoglou},
		    title = 	 {Metamodel-Assisted Evolution Strategies},
		      booktitle = 	 {Parallel Problem Solving from Nature - PPSN VII, 7th International Conference, Granada, Spain, September 7-11, 2002. Proceedings },
		      pages = 	 { 361 ff.},
  year = 	 {2002},
  editor = 	 {J.-J. Merelo Guerv\'os, P. Adamidis, H.-G. Beyer, J.-L. Fern\'andez-Villaca\~nas, H.-P. Schwefel},
  number = 	 {2439},
  series = 	 {Lecture Notes in Computer Science, LNCS},
  publisher = {Springer-Verlag},
  note = 	 {Keywords: Application::Aerospace, Technique::Advanced techniques - miscellaneous, Technique::Evolution strategies - floating-point, Technique::Fitness - Evaluation, Technique::Selection},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900361.pdf },
  abstract = {The paper presents various Metamodel-Assisted Evolution 
Strategies, which reduce the computational cost of 
optimisation problems involving time-consuming function 
evaluations. The metamodel is built from previously 
evaluated solutions in the search space and utilized 
to predict the fitness of new candidate solutions. 
In addition to previous works by the authors the new 
metamodel takes also account the error associated with 
each prediction, by correlating neighboring points in 
the search space. A mathematical problem and the problem 
of designing an optimal airfoil shape under viscious flow 
considerations have been worked out. Both demonstrate the 
gain in computational time the use of metamodels in Evolution 
strategies may offer.}
}
