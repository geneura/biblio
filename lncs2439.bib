  publisher = {Springer-Verlag},
  note = 	 {Keywords: Technique::Advanced techniques - miscellaneous, Technique::Genetic programming - general, Theory of EC::Evolution dynamics},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900341.pdf },
  abstract = {This paper presents a survey and comparison of significant
diversity measures in the genetic programming literature.
This study builds on previous work by the authors to gain 
a deeper understanding of the conditions under which genetic
programming evolution is successful. Three benchmark 
problems (Artificial Ant, Symbolic Regression and 
Even-5-Parity) are used to illustrate different diversity
measures and to analyse their correlation with performance.  
Results show that measures of population diversity based 
on edit distances and phenotypic diversity suggest that 
successful evolution occurs when populations converge to 
a similar structure but with high fitness diversity.}
}
@InProceedings{ LNCS2439:ID185:pp351,
		  author = 	 { Ying-ping Chen and David E. Goldberg},
		    title = 	 {Introducing Start Expression Genes to the Linkage Learning Genetic Algorithm},
		      booktitle = 	 {Parallel Problem Solving from Nature - PPSN VII, 7th International Conference, Granada, Spain, September 7-11, 2002. Proceedings },
		      pages = 	 { 351 ff.},
  year = 	 {2002},
  editor = 	 {J.-J. Merelo Guerv\'os, P. Adamidis, H.-G. Beyer, J.-L. Fern\'andez-Villaca\~nas, H.-P. Schwefel},
  number = 	 {2439},
