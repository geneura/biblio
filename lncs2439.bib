  note = 	 {Keywords: Activities::Routing and layout, Application::Transportation, Implementation::Parallel EAs, Related::Multi-agent Systems, Technique::Co-evolution, Technique::Multi-objective},
		 annote = {Available from http://link.springer.de/link/service/series/0558/papers/2439/243900871.pdf },
